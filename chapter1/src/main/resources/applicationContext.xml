<?xml version="1.0" encoding="utf-8" ?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans
           http://www.springframework.org/schema/beans/spring-beans-3.0.xsd">
    <bean id="connectionMaker" class="com.byultudy.user.dao.DConnectionMaker" />
    <bean id="countingConnectionMaker" class="com.byultudy.user.dao.CountingConnectionMaker" >
        <property name="connectionMaker" ref="connectionMaker" />
    </bean>
    <bean id="userDao" class="com.byultudy.user.dao.UserDao" >
        <property name="connectionMaker" ref="countingConnectionMaker" />
    </bean>
</beans>
